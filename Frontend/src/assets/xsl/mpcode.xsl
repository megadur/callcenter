<xsl:template match="node()|@*" mode="MP">
 <xsl:apply-templates select="node()|@*" mode="MP"/>
</xsl:template>

<xsl:template match="mp_mat_nr/text()[substring(.,11) = document('')/*/mp:codes/*/@key]" mode="MP">
 <xsl:variable name="vCur" select="substring(.,11)"/>
 <xsl:for-each select="document('')">
  <xsl:value-of select="$vCur"/>=<xsl:value-of select="key('kCodeByName', $vCur)/@value"/><br/>
 </xsl:for-each>
</xsl:template>

</xsl:stylesheet>
